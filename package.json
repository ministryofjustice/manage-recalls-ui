{
  "name": "manage-recalls-ui",
  "version": "0.0.1",
  "description": "Manage Recalls UI",
  "repository": "git@github.com:ministryofjustice/manage-recalls-ui.git",
  "license": "MIT",
  "scripts": {
    "copy-views": "cp -R server/views dist/server/",
    "compile-sass": "sass --no-source-map --load-path=node_modules/govuk-frontend --load-path=node_modules/@ministryofjustice/frontend ./assets/sass/application.sass:./assets/stylesheets/application.css ./assets/sass/application-ie8.sass:./assets/stylesheets/application-ie8.css --style compressed",
    "build": "npm run clean && npm run compile-sass && tsc && npm run copy-views",
    "local:build": "npm install && npm run build && npm run lint && npm run test && npm run pact-test && ./scripts/start-for-integration-tests.sh && npm run int-test && ./scripts/stop-local.sh",
    "start": "node $NODE_OPTIONS dist/server.js | bunyan -o short",
    "start:dev": "concurrently -k -p \"[{name}]\" -n \"Node,Styles\" -c \"yellow.bold,cyan.bold\" \"node -r ts-node/register/transpile-only --inspect=0.0.0.0 server.ts\" \"npm run compile-sass -- --watch\"",
    "start:e2e": "export $(cat e2e.env) && npm run start:dev",
    "start:feature": "export $(cat feature.env) && npm run build && npm run start",
    "kill": "npx kill-port 3000,9229",
    "record-build-info": "node ./bin/record-build-info",
    "swagger-to-ts": "node ./scripts/swagger-to-ts.js",
    "lint": "eslint . --cache --max-warnings 0",
    "lint:fix": "eslint . --cache --max-warnings 0 --fix",
    "typecheck": "tsc",
    "test": "jest --forceExit",
    "test:coverage": "jest --forceExit --coverage",
    "security_audit": "npx audit-ci --config audit-ci.json --skip-dev",
    "int-test": "rm -rf cypress/downloads && cypress run",
    "int-test-ui": "rm -rf cypress/downloads && cypress open",
    "clean": "rm -rf dist build",
    "prepare": "npx husky install",
    "pact-test": "rm -rf pact/pacts/* && jest --runInBand --forceExit --config=./pact/jest.pact.config.js"
  },
  "engines": {
    "node": "16.12.0",
    "npm": "6.14.13"
  },
  "config": {
    "pact_do_not_track": true
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverageFrom": [
      "server/**/*.{ts,js,jsx,mjs}"
    ],
    "coveragePathIgnorePatterns": [
      "@types",
      "<rootDir>/server/clients/manageRecallsApi/manageRecallsApiClient.ts",
      "<rootDir>/server/data/restClient.ts"
    ],
    "maxWorkers": 1,
    "testMatch": [
      "<rootDir>/(server|job)/**/?(*.)(spec|test).{ts,js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test_results/jest/"
        }
      ],
      [
        "./node_modules/jest-html-reporter",
        {
          "outputPath": "test_results/unit-test-reports.html"
        }
      ]
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ]
  },
  "lint-staged": {
    "*.{ts,js,css}": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.{json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ministryofjustice/frontend": "^1.0.0",
    "@opentelemetry/api": "^1.0.3",
    "@sentry/node": "^6.13.3",
    "@sentry/tracing": "^6.13.3",
    "accessible-autocomplete": "^2.0.3",
    "agentkeepalive": "^4.1.4",
    "applicationinsights": "^2.1.8",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.15",
    "bunyan-format": "^0.2.1",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "connect-redis": "^6.0.0",
    "cookie-session": "^1.4.0",
    "csurf": "^1.11.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-prom-bundle": "^6.4.1",
    "express-request-id": "^1.4.1",
    "express-session": "^1.17.2",
    "form-data": "^4.0.0",
    "google-libphonenumber": "^3.2.24",
    "govuk-frontend": "^3.14.0",
    "helmet": "^4.6.0",
    "http-errors": "^1.8.0",
    "jquery": "^3.6.0",
    "jwt-decode": "^3.1.2",
    "luxon": "^2.0.2",
    "multer": "^1.4.3",
    "nocache": "^3.0.1",
    "nunjucks": "^3.2.3",
    "nunjucks-date": "^1.5.0",
    "passport": "^0.5.0",
    "passport-oauth2": "^1.6.1",
    "prom-client": "^14.0.0",
    "redis": "^3.1.2",
    "superagent": "^6.1.0",
    "ts-node": "^10.2.1",
    "web-vitals": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@jest-mock/express": "^1.4.5",
    "@pact-foundation/pact": "^9.16.4",
    "@types/bunyan": "^1.8.7",
    "@types/bunyan-format": "^0.2.4",
    "@types/compression": "^1.7.2",
    "@types/connect-flash": "0.0.37",
    "@types/connect-redis": "^0.0.17",
    "@types/cookie-session": "^2.0.43",
    "@types/csurf": "^1.11.2",
    "@types/express-request-id": "^1.4.3",
    "@types/express-session": "^1.17.4",
    "@types/google-libphonenumber": "^7.4.21",
    "@types/http-errors": "^1.8.1",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/luxon": "^2.0.5",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.10.3",
    "@types/nunjucks": "^3.2.0",
    "@types/nunjucks-date": "0.0.7",
    "@types/passport": "^1.0.7",
    "@types/passport-oauth2": "^1.4.11",
    "@types/redis": "^2.8.32",
    "@types/superagent": "^4.1.13",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@typescript-eslint/typescript-estree": "^5.4.0",
    "concurrently": "^6.3.0",
    "cypress": "^8.7.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-multi-reporters": "^1.5.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "fs": "0.0.1-security",
    "husky": "^7.0.2",
    "jest": "^27.2.5",
    "jest-html-reporter": "^3.4.1",
    "jest-junit": "^13.0.0",
    "jest-pact": "^0.9.1",
    "jsonwebtoken": "^8.5.1",
    "kill-port": "^1.6.1",
    "lint-staged": "^11.2.3",
    "mkdirp": "^1.0.4",
    "mocha": "^9.1.2",
    "mocha-junit-reporter": "^2.0.2",
    "nock": "^13.1.3",
    "node-notifier": ">=10.0.0",
    "openapi-typescript-codegen": "^0.11.2",
    "pdf-parse": "^1.1.1",
    "prettier": "^2.4.1",
    "sass": "^1.42.1",
    "supertest": "^6.1.6",
    "tar": "^6.1.11",
    "ts-jest": "^27.0.5",
    "typescript": "^4.5.2",
    "wiremock-rest-client": "^1.9.0",
    "y18n": ">=5.0.8"
  }
}
