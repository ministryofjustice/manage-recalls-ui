<h2 class='govuk-heading-m govuk-!-margin-top-9' id='documents'>Recall documents</h2>
<dl class="govuk-summary-list">
    {% for document in recall.appGeneratedDocuments %}
        <div class="govuk-summary-list__row">
            <dt class=" govuk-summary-list__key">
                {{ document.label }}
            </dt>
            <dd class=" govuk-summary-list__value">
            {% if document.length %}
                <a class='govuk-link' href='{{ document.url }}' target='_blank' data-qa='appGeneratedDocuments-{{ document.category }}'>{{ person.firstName.toUpperCase() }}_{{ person.lastName.toUpperCase() }}_{{ document.fileName }}</a>
            {% else %}
                 <span data-qa='appGeneratedDocuments-{{ document.category }}'>Not available</span>
            {% endif %}
            </dd>
        </div>
    {% endfor %}
</dl>